{
  "redirects": [
    {
      "source": "/:path(.*)\\.html",
      "destination": "/:path",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/:path((?!.*\\.).*)",
      "destination": "/:path.html"
    }
  "redirects": [
{
  "trailingSlash": true,
  "cleanUrls": true
}
  ],
    
}

